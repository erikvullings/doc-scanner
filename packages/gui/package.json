{
  "name": "local-documents-library-client",
  "version": "0.0.1",
  "description": "Scan one or more folders and show the (office/PDF) documents in your browser. Supports comments and tagging.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./.cache",
    "link": "npm link mithril-ui-form",
    "start": "parcel index.html --out-dir ../server/public",
    "old:build:domain": "npm run clean && parcel build index.html --out-dir ../server/public --public-url https://erikvullings.github.io/mithril-materialized",
    "build:domain": "npm run clean && parcel build index.html --out-dir ../server/public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erikvullings/doc-scanner.git"
  },
  "keywords": [
    "library",
    "scanner",
    "office",
    "pdf",
    "docs",
    "webapp",
    "spa",
    "database"
  ],
  "author": "Erik Vullings <erik.vullings@gmail.com> (http://www.tno.nl)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erikvullings/doc-scanner/issues"
  },
  "homepage": "https://github.com/erikvullings/doc-scanner#readme",
  "dependencies": {
    "leaflet": "^1.5.1",
    "leaflet-draw": "^1.0.4",
    "marked": "^0.7.0",
    "material-icons": "^0.3.1",
    "materialize-css": "^1.0.0",
    "mithril": "^2.0.3",
    "mithril-leaflet": "^0.1.5",
    "mithril-materialized": "^0.13.15",
    "mithril-ui-form": "^0.2.0",
    "pdfjs-dist": "^2.1.266",
    "slimdown-js": "0.1.4",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@types/marked": "^0.6.5",
    "@types/materialize-css": "1.0.2",
    "@types/mithril": "^2.0.0",
    "@types/pdfjs-dist": "^2.0.1",
    "autoprefixer": "^9.6.1",
    "cssnano": "^4.1.10",
    "parcel-bundler": "^1.12.3",
    "rimraf": "^2.6.3",
    "typescript": "^3.5.3"
  }
}
